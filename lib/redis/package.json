    "@kelchy/log": "^1.0.3",
    "ioredis": "^4.27.7"
  },
  "devDependencies": {
    "@kelchy/pre-commit": "^1.2.4",
    "eslint": "^7.24.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "fengari": "^0.1.4",
    "fengari-interop": "^0.1.2",
    "ioredis-mock": "^5.6.0",
    "jest": "^26.6.3",
    "standard-as-callback": "^2.1.0"
