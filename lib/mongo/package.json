{
  "name": "@kelchy/mongo",
  "version": "0.1.2",
  "scripts": {
    "test:unit": "jest tests",
    "lint": "eslint ."
  },
  "pre-commit": {
    "run": [
      "lint",
      "test:unit"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "bail": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "main": "./index.js",
  "license": "GPLv3",
  "dependencies": {
    "mongodb": "^4.1.1"
  },
  "devDependencies": {
    "@kelchy/pre-commit": "^1.2.4",
    "eslint": "^7.24.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "jest": "^26.6.3"
  }
}
